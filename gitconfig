[alias]
    cl = clone
    co = checkout
    ci = commit
    st = status
    br = branch
    lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    lc = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit -p
    type = cat-file -t
    dump = cat-file -p
    open = !hub browse -- 
    issues = !hub browse -- issues
    pulls = !hub browse -- pulls
    settings = !hub browse -- settings [user]
